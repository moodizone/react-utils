{
  "name": "@moodizone_org/react-utils",
  "version": "0.0.1",
  "description": "Bunch of utils for react applications",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "hooks",
    "utilities"
  ],
  "files": [
    "dist"
  ],
  "author": {
    "name": "PayPing",
    "email": "moodizone.org@gmail.com",
    "url": "https://github.com/moodizone/react-utils"
  },
  "scripts": {
    "build": "",
    "lint": "eslint . --ext .tsx,.jsx,.js,ts,.json --ignore-path .gitignore",
    "prettier": "prettier \"**.(js|ts|tsx|jsx|json)\" --ignore-path .gitignore",
    "check-types": "tsc",
    "check-format": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "validate": "npm run check-format && npm run lint",
    "rollup": "rm dist & rollup -c",
    "prepare": "husky install"
  },
  "repository": {
    "url": "https://github.com/moodizone/react-utils"
  },
  "dependencies": {
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@types/react": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "@rollup/plugin-commonjs": "^20.0.0",
    "rollup": "^2.58.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17.0.2"
  },
  "lint-staged": {
    "*.{tsx,jsx}": "npm run validate"
  }
}
